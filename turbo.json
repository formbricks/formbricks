{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": [
        "GITHUB_ID",
        "GITHUB_SECRET",
        "MAIL_FROM",
        "NEXT_PUBLIC_EMAIL_VERIFICATION_DISABLED",
        "NEXT_PUBLIC_PASSWORD_RESET_DISABLED",
        "NEXT_PUBLIC_PRIVACY_URL",
        "NEXT_PUBLIC_SIGNUP_DISABLED",
        "NEXT_PUBLIC_TERMS_URL",
        "NEXTAUTH_SECRET",
        "NEXTAUTH_URL",
        "SMTP_HOST",
        "SMTP_PASSWORD",
        "SMTP_PORT",
        "SMTP_SECURE_ENABLED",
        "SMTP_USER",
        "NEXT_PUBLIC_DOCSEARCH_APP_ID",
        "NEXT_PUBLIC_DOCSEARCH_API_KEY",
        "NEXT_PUBLIC_DOCSEARCH_INDEX_NAME",
        "NODE_ENV"
      ]
    },
    "db:migrate:deploy": {
      "outputs": []
    },
    "db:push": {
      "outputs": []
    },
    "db:seed": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "generate": {
      "dependsOn": ["^generate"]
    },
    "lint": {
      "outputs": []
    }
  }
}
