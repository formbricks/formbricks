{
  "$schema": "https://lingo.dev/schema/i18n.json",
  "buckets": {
    "json": {
      "include": ["locales/[locale].json"]
    }
  },
  "locale": {
    "source": "en-US",
    "targets": ["en-US", "de-DE", "fr-FR", "ja-JP", "pt-BR", "pt-PT", "ro-RO", "zh-Hans-CN", "zh-Hant-TW", "nl-NL"]
  },
  "version": 1.8
}
