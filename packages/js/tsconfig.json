{
  "extends": "@formbricks/config-typescript/js-library.json",
  "include": ["src", "package.json"],
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "paths": {
      "@formbricks/js-core/*": ["../js-core/dist/*"]
    }
  }
}
