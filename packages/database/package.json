    "prebuild": "npm run generate",
    "predev": "npm run generate",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1"
  },
  "devDependencies": {
    "@formbricks/tsconfig": "workspace:*",
    "eslint": "^8.27.0",
    "eslint-config-formbricks": "workspace:*",
    "prisma": "^4.6.1",
    "prisma-dbml-generator": "^0.10.0",
    "rimraf": "^3.0.2",
    "tsup": "^6.4.0",
    "tsx": "^3.12.0",
    "typescript": "^4.8.4"
  }
